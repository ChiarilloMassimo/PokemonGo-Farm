<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>PokemonGo - Farm</title>

    <!-- Bootstrap Core CSS -->
    <link href="/components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="/components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">

    <!-- Timeline CSS -->
    <link href="/css/timeline.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="/css/sb-admin-2.css" rel="stylesheet">

    <!-- App -->
    <link href="/css/app.css" rel="stylesheet">

    <!-- Select 2 -->
    <link href="/components/select2/dist/css/select2.min.css" rel="stylesheet">
    <link href="/components/select2-bootstrap-theme/dist/select2-bootstrap.min.css" rel="stylesheet">

    <!-- Morris Charts CSS -->
    <link href="/components/morrisjs/morris.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

<div id="wrapper">

    <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('dashboard') }}">PokemonGo Farm</a>
        </div>

        <div class="navbar-default sidebar" role="navigation">
            <div class="sidebar-nav navbar-collapse">
                <ul class="nav" id="side-menu">
                    <li>
                        <a href="{{ path('dashboard') }}"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-male fa-fw"></i> Bot<span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="{{ path('bot_new') }}">New Config</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#"><i class="fa fa-warning fa-fw"></i> Logs</a>
                    </li>
                    <li>
                        <a href="https://github.com/ChiarilloMassimo/PokemonGo-Farm" target="_blank"><i class="fa fa-github fa-fw"></i> Info</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">{% block page_title %}{% endblock %}</h1>
            </div>
        </div>
        {% block content %}{% endblock %}
    </div>
</div>

{% block javascripts %}
    <!-- jQuery -->
    <script src="/components/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Select 2 -->
    <script src="/components/select2/dist/js/select2.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="/components/metisMenu/dist/metisMenu.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="/components/morrisjs/morris.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="/js/sb-admin-2.js"></script>

    <!-- Google Maps -->
    <script src="http://maps.googleapis.com/maps/api/js?key={{ gmap_api_key }}&libraries=places"></script>

    <!-- JQuery GeoComplete -->
    <script src="/components/geocomplete/jquery.geocomplete.min.js"></script>
{% endblock %}
</body>

</html>
